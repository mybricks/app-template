{
  "types": [
    "node"
  ],
  "compilerOptions": {
    "module": "es2020",
    "target": "es6",
    "lib": [
      "dom",
      "es6",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@sdk": [
        "../nocode-engine/desn-sdk/src/index.ts"
      ],
      "@utils": [
        "../nocode-engine/desn-sdk/src/utils/index.ts"
      ],
      "@mybricks/rxui": [
        "../rxui/types.d.ts"
      ],
      "@mybricks/file-parser": [
        "../file-parser/src/index.ts"
      ],
      "@mybricks/editors-pc-common": [
        "../editors-pc-common/src"
      ],
      "@mybricks/desn-edit-view": [
        "../nocode-engine/desn-edit-view/src"
      ],
      "@mybricks/desn-geo-view": [
        "../nocode-engine/desn-geo-view/src/index.ts"
      ],
      "@mybricks/desn-topl-view": [
        "../nocode-engine/desn-topl-view/src/index.ts"
      ],
      "@mybricks/comlib-core": [
        "../_comlibs/comlib-core/src/runtime.ts"
      ]
    }
  },
  "exclude": [
    "./node_modules"
  ]
}
